{
   "name": "mimic/cctools",
   "description": "基于Laravel的开源工具库",
   "authors": [
      {
         "name": "shine",
         "email": "10670374@qq.com"
      }
   ],
   "autoload": {
      "psr-4": {
         "CCTools\\": "src/"
      }
   },
   "require": {
      "php": "^7.2|^8.0",
      "illuminate/support": "5.*|6.*|7.*|8.*",
      "ramsey/uuid": "^4.1",
      "godruoyi/php-snowflake": "^1.0"
   },
   "require-dev": {
      "phpunit/phpunit": "~5.0"
   },
   "license": "MIT",
   "extra": {
      "laravel": {
         "providers": [
            "CCTools\\Providers\\RsaUtilServiceProvider",
            "CCTools\\Providers\\SnowFlakeServiceProvider"
         ],
         "aliases": {
            "RsaUtil": "RsaUtil",
            "SnowFlake": "SnowFlake"
         }
      }
   }
}
